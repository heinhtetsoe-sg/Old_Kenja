  <Text name="Text22" x="492" y="3435" strText="　各学期の学習目標への到達度を、教科ごとに３つの観点で評価しています。" pitch="2.0457516339869244" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text23" x="432" y="3553" strText="【よくできる】：到達目標を十分に達成している。習熟している。" pitch="1.5632183908045951" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
