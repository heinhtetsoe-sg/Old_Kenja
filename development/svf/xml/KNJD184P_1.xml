  <Text name="Text2" x="1426" y="567" strText="通知表の見方" pitch="0" fixPitch="false" fontNum="0" hanFontNum="0" point="16" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text3" x="432" y="754" strText="　通知表は、学校でのお子さまの生活や学習の様子をお知らせし、家庭と学校が協力" pitch="2.8078078078078086" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text4" x="433" y="837" strText="して、望ましい成長・発達のために役立つことを願って作成しています。学校での" pitch="3.776234567901232" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text5" x="432" y="915" strText="様子と育ちゆく姿を見ていただき、未来に向かって伸びようとするお子さまを励まし、" pitch="2.350877192982454" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text6" x="432" y="993" strText="元気づけ、やる気を持たせる資料としてご活用ください。" pitch="2.5422222222222217" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text7" x="355" y="1183" strText="１．生活" pitch="0" fixPitch="false" fontNum="0" hanFontNum="0" point="14" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text8" x="493" y="1347" strText="「生活」は、学期ごとに特に優れている項目や努力が認められる項目に○をつけて" pitch="2.9429012345678984" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text9" x="492" y="1427" strText="います。友だちとの比較ではなく、お子さまの良い点や努力を褒めてあげてください。" pitch="1.6929824561403488" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text10" x="355" y="1615" strText="２．特別活動" pitch="0" fixPitch="false" fontNum="0" hanFontNum="0" point="14" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text11" x="492" y="1780" strText="　クラスでの係活動、児童会活動における委員会（５、６年生のみ）、各種コンクール、" pitch="0.609686609686611" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text12" x="492" y="1859" strText="スポーツ大会等での入賞歴などを記載しています。" pitch="1.828282828282826" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text13" x="355" y="2049" strText="３．明小タイム" pitch="0" fixPitch="false" fontNum="0" hanFontNum="0" point="14" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text14" x="492" y="2215" strText="　明小タイムでの活動の様子を、年間に一度文章で記載いたします。" pitch="2.3259259259259237" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text15" x="355" y="2403" strText="４．出欠の記録" pitch="0" fixPitch="false" fontNum="0" hanFontNum="0" point="14" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text16" x="493" y="2569" strText="　１学期は６月末日まで、２学期は７月を含めて１１月まで、３学期は１２月を含め" pitch="2.0375375375375384" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text17" x="492" y="2647" strText="て２月までの出席、欠席の記録です。３月は入りません。" pitch="2.142222222222222" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text18" x="355" y="2837" strText="５．生活や学習について気がついたこと" pitch="0" fixPitch="false" fontNum="0" hanFontNum="0" point="14" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text19" x="493" y="3001" strText="　１、２学期は個人面談を実施いたしますので記載はありません。３学期に、１年間" pitch="2.0375375375375384" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
  <Text name="Text20" x="492" y="3081" strText="を通したお子さまの様子を総合的に見ての所見を記載いたします。" pitch="2.2183908045976986" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
