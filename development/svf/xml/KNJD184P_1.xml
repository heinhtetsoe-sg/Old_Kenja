  <Text name="Text25" x="432" y="3711" strText="【もう少し】：基礎・基本の定着に努力を必要とする。" pitch="1.4212962962962952" fixPitch="false" fontNum="0" hanFontNum="0" point="11" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="false" layerNumber="30" locked="false" groupIndex="0" pageFlags="true" colorIndexes="0" />
