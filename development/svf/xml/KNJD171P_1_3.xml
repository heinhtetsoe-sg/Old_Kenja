  <Text name="Text11" x="571" y="3920" strText="や成長のあとを確かめて、これから先の目標や努力の手がかりにして欲しいと願って" pitch="0.768768768768766" fixPitch="false" fontNum="0" hanFontNum="0" point="10" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="true" layerNumber="12" locked="false" groupIndex="0" colorIndexes="0" />
  <Text name="Text12" x="571" y="3988" strText="います。" pitch="0" fixPitch="false" fontNum="0" hanFontNum="0" point="10" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="true" layerNumber="12" locked="false" groupIndex="0" colorIndexes="0" />
  <Text name="Text13" x="571" y="4060" strText="　評価は「◎…よくできた」、「○…できた」、「△…もう少し」で記してあります。" pitch="2.013157894736839" fixPitch="false" fontNum="0" hanFontNum="0" point="9.3" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="true" layerNumber="12" locked="false" groupIndex="0" colorIndexes="0" />
  <Text name="Text14" x="571" y="4128" strText="　評定（３，２，１）は他と比べる評価ではなく、目標に対してどれだけできたかを" pitch="2.7657657657657637" fixPitch="false" fontNum="0" hanFontNum="0" point="9.3" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="true" layerNumber="12" locked="false" groupIndex="0" colorIndexes="0" />
  <Text name="Text15" x="571" y="4198" strText="判断したものになっています。「生活のようす」は、一人一人の子どもを励まし、さら" pitch="2.013157894736839" fixPitch="false" fontNum="0" hanFontNum="0" point="9.3" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="true" layerNumber="12" locked="false" groupIndex="0" colorIndexes="0" />
  <Text name="Text16" x="571" y="4266" strText="に成長して欲しいという願いを込めて評価しています。" pitch="2.506944444444443" fixPitch="false" fontNum="0" hanFontNum="0" point="9.3" vertRatio="1" vertRatioN="1" horzRatio="1" horzRatioN="1" direction="0" rotation="0" frmCompatible="true" graphicMode="false" bold="false" italic="false" outline="false" outlineWidth="1" strImage="" whiteChar="false" shadow="false" useGradientPaint="false" colorGradient1="1" colorGradient2="1" gradientKind="0" textColor="1" commonPage="true" layerNumber="12" locked="false" groupIndex="0" colorIndexes="0" />
